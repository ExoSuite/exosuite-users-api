{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "ext-xml": "*",
        "bensampo/laravel-enum": "^1.12",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/scout": "^5.0",
        "laravel/tinker": "^1.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.2",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "darkaonline/l5-swagger": "5.7.*",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "if [ -f ./vendor/bin/phpunit ]; then php artisan ide-helper:generate; fi",
            "if [ -f ./vendor/bin/phpunit ]; then php artisan ide-helper:meta; fi",
            "if [ -f ./vendor/bin/phpunit ]; then php artisan ide-helper:models; fi",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "reformat": "./vendor/bin/phpcbf ./app ./tests ./database ./routes --standard=PSR12"
    }
}
